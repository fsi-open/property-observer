    "keywords": ["fsi", "component", "property observer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/property-access": "^2.3|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "FSi\\Component\\PropertyObserver": "lib/",
            "FSi\\Component\\PropertyObserver\\Tests": "tests/"
        }
